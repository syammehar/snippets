services.AddCors(options =>
            {
                options.AddPolicy(name: "WasmPolicy",
                    builder =>
                    {
                        builder.WithOrigins("https://localhost:44314",
                            "https://localhost:5001")
                        .WithMethods("GET", "POST", "DELETE");
                    });
            });
            
app.UseCors("WasmPolicy");
